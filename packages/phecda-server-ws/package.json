{
  "name": "phecda-server-ws",
  "version": "1.0.2",
  "description": "provide ws function to phecda-server",
  "author": "fgsreally",
  "license": "MIT",
  "repository": "https://github.com/fgsreally/phecda-ws/tree/main/packages/phecda-server-ws",
  "keywords": ["ws", "phecda-server"],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./ioredis": {
      "require": "./dist/ioredis/index.js",
      "import": "./dist/ioredis/index.mjs"
    }
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "dist/index.d.ts"
      ],
      "./ioredis": [
        "dist/ioredis/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup"
  },
  "dependencies": {
    "debug": "^4.3.4",
    "nanoid": "^5.0.7",
    "phecda-server": "^5.2.0",
    "ws": "^8.17.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/ws": "^8.5.10",
    "ioredis": "^5.4.1",
    "tsup": "^8.0.2"
  }
}
